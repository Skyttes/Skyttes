{
    "name": "skyttes/skyttes",
    "description": "Skyttes Core",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mia Pilchov√°",
            "email": "pilchova@lilianaa.dev"
        }
    ],
    "require": {
        "doctrine/dbal": "^3.6",
        "doctrine/orm": "^2.15",
        "nette/application": "^3.1",
        "nette/bootstrap": "^3.2",
        "nette/di": "^3.1",
        "nette/forms": "^3.1",
        "nette/security": "^3.1",
        "nette/utils": "^4.0",
        "ramsey/uuid": "^4.7",
        "ramsey/uuid-doctrine": "^2.1",
        "symfony/string": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Skyttes\\": "src/"
        }
    },
    "replace": {
        "skyttes/core": "self.version",
        "skyttes/doctrine": "self.version",
        "skyttes/security": "self.version",
        "skyttes/vite": "self.version"
    },
    "suggest": {
        "ext-imagick": "Required for Images::stripImage()",
        "ext-curl": "Required for Vite Dev Server handling",
        "icamys/php-sitemap-generator": "For sitemap generation"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
